{
  "name": "@cosmicvelocity/commons-js",
  "version": "0.1.2",
  "description": "It provides reusable components with small granularity.",
  "keywords": [
    "es6",
    "modules",
    "util"
  ],
  "author": "Kouichi Machida <k-machida@aideo.co.jp>",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://github.com/cosmicvelocity/commons-js#readme",
  "bugs": {
    "url": "https://github.com/cosmicvelocity/commons-js/issues"
  },
  "main": "build/src/index.js",
  "files": [
    "build"
  ],
  "directories": {
    "lib": "build"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "test": "cross-env NODE_ENV=test jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmicvelocity/commons-js.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/moment": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "moment": "^2.24.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "ie >= 11",
    "Chrome >= 21",
    "Firefox >= 28",
    "Safari >= 6.1",
    "Opera >= 12.1",
    "iOS >= 7.1",
    "Android >= 4.4"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "testRegex": "/test/.*\\.ts$",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "verbose": true
  }
}
